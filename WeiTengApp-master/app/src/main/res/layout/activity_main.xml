<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.weiteng.weitengapp.ui.activity.MainActivity">
    <com.weiteng.weitengapp.ui.view.IconViewPager
        android:id="@+id/activity_main_ip"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:background="@android:color/white"
        android:layout_alignParentBottom="true"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/activity_main_vp"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:layout_above="@id/activity_main_ip"/>
</RelativeLayout>
