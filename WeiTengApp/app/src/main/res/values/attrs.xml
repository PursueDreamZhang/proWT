<resources>

    <!-- IconViewPager -->
    <declare-styleable name="TabView">
        <attr name="tabView" format="reference" />
    </declare-styleable>
    <declare-styleable name="SlideMode">
        <attr name="mode">
            <enum name="forbid" value="0"></enum>
            <enum name="left" value="1"></enum>
            <enum name="right" value="2"></enum>
            <enum name="both" value="3"></enum>
        </attr>
    </declare-styleable>

</resources>